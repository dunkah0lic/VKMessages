<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <Button
        android:id="@+id/authorize"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Авторизация"
    />
    
    <EditText
        android:id="@+id/message"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Тестовая запись на стене"
    />
    <Button
        android:id="@+id/post"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Отправить запись на стену"
    />
    
    <Button
        android:id="@+id/logout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Выход"
    />

</LinearLayout><!-- From: file:/C:/Users/Nikolay/AndroidStudioProjects/VKMessages4/androidVkSdk/src/main/res/layout/main.xml --><!-- From: file:/C:/Users/Nikolay/AndroidStudioProjects/VKMessages4/vKMessages/build/intermediates/exploded-aar/VKMessages4/androidVkSdk/unspecified/res/layout/main.xml -->